{
  "name": "web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "bun run build && wrangler pages deploy .svelte-kit/cloudflare --project-name=supasalon-web"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.1",
    "@lucide/svelte": "^0.561.0",
    "@repo/constants": "workspace:*",
    "@schedule-x/calendar": "^2.2.0",
    "@schedule-x/svelte": "^2.2.0",
    "@schedule-x/theme-default": "^2.2.0",
    "better-auth": "^1.4.18",
    "mode-watcher": "^1.1.0",
    "svelte-sonner": "^1.0.7",
    "temporal-polyfill": "^0.2.5"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.6",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "bits-ui": "^2.15.4",
    "clsx": "^2.1.1",
    "postcss": "^8.5.6",
    "shadcn-svelte": "^1.1.1",
    "svelte": "^5.49.0",
    "svelte-check": "^4.3.5",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wrangler": "^4.61.1"
  }
